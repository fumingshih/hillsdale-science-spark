                grade: 'Senior, Hillsdale High School',
                badge: 'Active Volunteer',
                bio: '[Add description - favorite activity, why they volunteer]',
                image: 'Replace with volunteer photo'
            }
            // Add more team members as needed
        ];

        let currentSlideIndex = 0;

        function renderCarousel() {
            const member = teamMembers[currentSlideIndex];
            const slideHTML = `
                <div class="carousel-image">
                    <div class="placeholder-img" style="height: 450px;">
                        ${member.image}
                    </div>
                </div>
                <div class="carousel-description">
                    <span class="member-badge">${member.badge}</span>
                    <h3>${member.name}</h3>
                    <p class="member-role">${member.grade}</p>
                    <p class="member-bio">${member.bio}</p>
                </div>
            `;

            const carouselSlide = document.querySelector('.carousel-slide');
            if (carouselSlide) {
                carouselSlide.innerHTML = slideHTML;
            }

            updateDots();
        }

        function moveCarousel(direction) {
            currentSlideIndex += direction;

            if (currentSlideIndex < 0) {
                currentSlideIndex = teamMembers.length - 1;
            } else if (currentSlideIndex >= teamMembers.length) {
                currentSlideIndex = 0;
            }

            renderCarousel();
        }

        function goToSlide(index) {
            currentSlideIndex = index;
            renderCarousel();
        }

        function updateDots() {
            const dots = document.querySelectorAll('.dot');
            dots.forEach((dot, index) => {
                if (index === currentSlideIndex) {
                    dot.classList.add('active');
                } else {
                    dot.classList.remove('active');
                }
            });
        }

        // Initialize carousel on page load
        if (document.querySelector('.carousel-slide')) {
            renderCarousel();

            // Update dots to match number of team members
            const dotsContainer = document.getElementById('carouselDots');
            if (dotsContainer) {
                dotsContainer.innerHTML = teamMembers.map((_, index) =>
                    `<span class="dot ${index === 0 ? 'active' : ''}" onclick="goToSlide(${index})"></span>`
                ).join('');
            }
        }
    </script>
